<h1>New location</h1>
<%= form_for (@location) do |f|%>
<%=render 'shared/error_messages', :object => f.object%>
  <div class = "field">
    <%= f.label :name%><br/>
    <%= f.text_field :name%>
  </div>
  <div class ="actions">
    <%=f.submit "Add"%>
  </div>
  <%end%>

<%= set_focus_to_id 'location_name' %>
